local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("Nick's UT:Boss Battles GUI", "DarkTheme")

local Tab = Window:NewTab("Info")
local Section = Tab:NewSection("Information")

Section:NewLabel("Priorities: These are how important the game views")
Section:NewLabel("a character. P1 being most important and usually")
Section:NewLabel("the first target.")

Section:NewLabel("Entirely Developed by NixelPixel/NICKISBAD")

Section:NewButton("Check me out! (Scriptblox)", "copies my scriptblox profile link", function()
setclipboard("https://scriptblox.com/u/NixelPixel")
end)



local Tab = Window:NewTab("Overpowered")

local Section = Tab:NewSection("Main")

Section:NewButton("God's SMITE! (Priority 1)", "I smite thee with thy power of then blessed one!", function()


local args = {
    [1] = "GasterBlaster",
    [2] = workspace.Game.Mobs.Dummy,
    [3] = "inf"
}


game:GetService("ReplicatedStorage").ArmorShoot:FireServer(unpack(args))

end)

Section:NewButton("God's SMITE! (Priority 2)", "I smite thee with thy power of then blessed one!", function()


local args = {
    [1] = "GasterBlaster",
    [2] = workspace.Game.Mobs.Dummy2,
    [3] = "inf"
}


game:GetService("ReplicatedStorage").ArmorShoot:FireServer(unpack(args))

end)

Section:NewButton("God's SMITE! (Priority 3)", "I smite thee with thy power of then blessed one!", function()


local args = {
    [1] = "GasterBlaster",
    [2] = workspace.Game.Mobs.Dummy3,
    [3] = "inf"
}


game:GetService("ReplicatedStorage").ArmorShoot:FireServer(unpack(args))

end)

Section:NewButton("Infinite HP", "Gives you inf HP", function()
-- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "DamageTaken",
    [2] = false,
    [3] = -"inf"
}

game:GetService("ReplicatedStorage").Functions:FireServer(unpack(args))
	
end)

local Tab = Window:NewTab("LocalPlayer")
local Section = Tab:NewSection("Modifications")

Section:NewSlider("WalkSpeed", "Sets the Player's Walkspeed", 100, 0, function(t)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = t
end)

Section:NewSlider("JumpPower", "Sets the Player's JumpPower", 100, 0, function(t)
game.Players.LocalPlayer.Character.Humanoid.JumpPower = t
end)

Section:NewTextBox("Give yourself HP", "Give yourself any amount of hp", function(f)

local args = {
    [1] = "DamageTaken",
    [2] = false,
    [3] = -f
}

game:GetService("ReplicatedStorage").Functions:FireServer(unpack(args))
	
end)

Section:NewButton("Instant Death", "Helps with glitches (DO NOT USE WITH INF HP", function()
game.Player.LocalPlayer.Character.Humanoid.Health = 0
end)

local Tab = Window:NewTab("Misc")
local Section = Tab:NewSection("KeyBinds")

Section:NewKeybind("Instant Kill Priority 1 ", "Kills first priority enemy (Bypasses Dodges)", Enum.KeyCode.P, function()
local hum = game.Workspace.Game.Mobs.Dummy.Enemy
hum.Health = 0
end)

Section:NewKeybind("Instant Kill Priority 2 ", "Kills second priority enemy (Bypasses Dodges)", Enum.KeyCode.P, function()
local hum = game.Workspace.Game.Mobs.Dummy2.Enemy
hum.Health = 0
end)

Section:NewKeybind("Instant Kill Priority 3 ", "Kills third priority enemy (Bypasses Dodges)", Enum.KeyCode.P, function()
local hum = game.Workspace.Game.Mobs.Dummy3.Enemy
hum.Health = 0
end)

local info = {
	enemy = "name"
}

Section:NewDropdown("Manual Instakill", "Manuality is fun", {"Dummy", "Dummy2", "Dummy3" }, function(msd)
local target = game.Workspace.Game.Mobs [msd] .Enemy
info.enemy = target
end)

Section:NewButton("Kill Selected", "Kills the Selected enemy", function()
info.enemy.Health = 0
end)

local Tab = Window:NewTab("Settings")

local Section = Tab:NewSection("Options")

Section:NewKeybind("GUI Toggle", "Toggles the GUI", Enum.KeyCode.RightControl, function()
	Library:ToggleUI()
end)
